<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    <style name="Divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_marginLeft">46dp</item>
        <item name="android:background">?android:attr/listDivider</item>
    </style>

    <style name="Big_Divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/third_text</item>
    </style>

    <style name="White_Divider">
        <item name="android:layout_width">130dp</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/colorWhite</item>
    </style>

    <style name="Hor_Divider">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_margin">5dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/third_text</item>
    </style>

</resources>
